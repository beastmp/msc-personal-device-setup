{
    "directories": {
        "binaries": "C:\\Testing\\Binaries",
        "scripts": "C:\\Temp\\Scripts",
        "staging": "C:\\Testing\\Staging",
        "install": "C:\\Testing\\Apps",
        "postInstall": "C:\\Testing\\Installed",
        "logs": "C:\\Testing\\Logs"
    },
    "files": {
        "softwareList": "personal_softwarelist.json"
    },
    "logging": {
        "fileNameFormat": "{LogType}-{ScriptName}-{Action}-{TargetName}-{Version}-{DateTime}.txt"
    },
    "execution": {
        "maxConcurrentJobs": 3,
        "jobTimeoutSeconds": 1800,
        "cacheRetentionDays": 30,
        "retryCount": 3,
        "retryDelaySeconds": 10
    },
    "cleanup": {
        "removeStaging": true,
        "removeFailedInstalls": true,
        "cleanupCache": true
    }
}
