{
    "directories": {
        "binaries": "D:\\Installation\\Binaries",
        "scripts": "D:\\Scripts",
        "staging": "D:\\Installation\\Staging",
        "install": "D:\\Apps",
        "postInstall": "D:\\Installation\\Installed",
        "logs": "D:\\Logs"
    },
    "files": {
        "softwareList": "softwarelist.json"
    },
    "logging": {
        "fileNameFormat": "{LogType}-{ScriptName}-{Action}-{TargetName}-{Version}-{DateTime}.txt"
    },
    "execution": {
        "maxConcurrentJobs": 3,
        "jobTimeoutSeconds": 1800,
        "cacheRetentionDays": 30,
        "retryCount": 3,
        "retryDelaySeconds": 10
    },
    "cleanup": {
        "removeStaging": false,
        "removeFailedInstalls": false,
        "cleanupCache": false
    }
}
